<LinearLayout android:layout_height="match_parent"
    android:layout_width="match_parent"
    xmlns:tools="http://schemas.android.com/tools"
    android:orientation="vertical"
    android:background="#E7E7E7"
    tools:context=".AdministratorActivity"
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto">
    <com.google.android.material.appbar.AppBarLayout
        android:id="@+id/Apvar"
        app:elevation="10dp"
        android:background="#F2472C"
        android:layout_width="match_parent"
        android:layout_height="50dp">
        <com.google.android.material.navigation.NavigationView
            android:background="#F2472C"
            android:elevation="10dp"
            android:layout_width="match_parent"
            android:layout_height="match_parent">
            <RelativeLayout
                android:id="@+id/admin_back_pressed"
                android:layout_width="60dp"
                android:layout_height="match_parent">
                <ImageView
                    android:layout_centerVertical="true"
                    android:layout_width="70dp"
                    android:layout_height="20dp"
                    android:fontFamily="@font/adamina"
                    android:src="@drawable/ic_baseline_arrow_back_ios_24"
                    android:textColor="#ffffff"
                    android:textStyle="bold"
                    android:textSize="18sp"/>
            </RelativeLayout>
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_gravity="center"
                android:fontFamily="@font/adamina"
                android:text="Administrator"
                android:textAlignment="center"
                android:textColor="#ffffff"
                android:textStyle="bold"
                android:textSize="18sp"/>
        </com.google.android.material.navigation.NavigationView>
    </com.google.android.material.appbar.AppBarLayout>
<!--    <TextView-->
<!--        android:textStyle="bold"-->
<!--        android:paddingBottom="10dp"-->
<!--        android:paddingTop="10dp"-->
<!--        android:layout_width="match_parent"-->
<!--        android:layout_height="wrap_content"-->
<!--        android:fontFamily="@font/adamina"-->
<!--        android:paddingStart="20dp"-->
<!--        android:text="Student"-->
<!--        android:textColor="@color/black"-->
<!--        android:textSize="18sp"-->
<!--        tools:ignore="RtlSymmetry" />-->
    <LinearLayout
        android:layout_marginTop="10dp"
        android:orientation="vertical"
        android:weightSum="3"
        android:layout_width="match_parent"
        android:layout_height="310dp">
        <LinearLayout
            android:orientation="horizontal"
            android:layout_weight="1"
            android:layout_width="match_parent"
            android:weightSum="3"
            android:layout_height="0dp">
            <androidx.cardview.widget.CardView
                app:cardElevation="10dp"
                android:layout_marginHorizontal="5dp"
                android:layout_width="0dp"
                android:layout_weight="1"
                android:layout_height="match_parent">
                <RelativeLayout
                    android:id="@+id/card_administor_notice"
                    android:background="@drawable/hover_effect"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"/>
                <ImageView
                    android:layout_marginTop="-10dp"
                    android:layout_gravity="center"
                    android:src="@drawable/notice"
                    android:layout_width="40dp"
                    android:layout_height="40dp"/>
                <TextView
                    android:layout_marginStart="-3dp"
                    android:textColor="#394D71"
                    android:textSize="14sp"
                    android:layout_marginTop="30dp"
                    android:layout_gravity="center"
                    android:textStyle="bold"
                    android:text="Notice"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"/>
            </androidx.cardview.widget.CardView>
            <androidx.cardview.widget.CardView
                app:cardElevation="10dp"
                android:layout_marginHorizontal="5dp"
                android:layout_width="0dp"
                android:layout_weight="1"
                android:layout_height="match_parent">
                <RelativeLayout
                    android:id="@+id/card_admistor_complaints"
                    android:background="@drawable/hover_effect"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"/>
                <ImageView
                    android:layout_marginTop="-10dp"
                    android:layout_gravity="center"
                    android:src="@drawable/notic"
                    android:layout_width="35dp"
                    android:layout_height="35dp"/>

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center"
                    android:layout_marginTop="30dp"
                    android:text="Complaint"
                    android:textColor="#394D71"
                    android:textSize="14dp"
                    android:textStyle="bold"
                    tools:ignore="TextSizeCheck" />
            </androidx.cardview.widget.CardView>
            <androidx.cardview.widget.CardView
                app:cardElevation="10dp"
                android:layout_marginHorizontal="5dp"
                android:layout_width="0dp"
                android:layout_weight="1"
                android:layout_height="match_parent">
                <RelativeLayout
                    android:id="@+id/card_admistor_timetable"
                    android:background="@drawable/hover_effect"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"/>
                <ImageView
                    android:layout_marginTop="-10dp"
                    android:layout_gravity="center"
                    android:src="@drawable/calendar"
                    android:layout_width="35dp"
                    android:layout_height="35dp"/>
                <TextView
                    android:textColor="#394D71"
                    android:textSize="14sp"
                    android:layout_marginTop="30dp"
                    android:layout_gravity="center"
                    android:textStyle="bold"
                    android:text="Timetable"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"/>
            </androidx.cardview.widget.CardView>
        </LinearLayout>
        <LinearLayout
            android:layout_marginTop="10dp"
            android:layout_weight="1"
            android:layout_width="match_parent"
            android:weightSum="3"
            android:layout_height="0dp"
            android:orientation="horizontal"
            >
            <androidx.cardview.widget.CardView
                app:cardElevation="10dp"
                android:layout_marginHorizontal="5dp"
                android:layout_width="0dp"
                android:layout_weight="1"
                android:layout_height="match_parent">
                <RelativeLayout
                    android:id="@+id/card_admistor_application"
                    android:background="@drawable/hover_effect"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"/>
                <ImageView
                    android:layout_marginTop="-10dp"
                    android:layout_gravity="center"
                    android:src="@drawable/apllication"
                    android:layout_width="35dp"
                    android:layout_height="35dp"/>

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center"
                    android:layout_marginTop="30dp"
                    android:text="Application"
                    android:textColor="#394D71"
                    android:textSize="14dp"
                    android:textStyle="bold"
                    tools:ignore="TextSizeCheck" />
            </androidx.cardview.widget.CardView>
            <androidx.cardview.widget.CardView
                app:cardElevation="10dp"
                android:layout_marginHorizontal="5dp"
                android:layout_width="0dp"
                android:layout_weight="1"
                android:layout_height="match_parent">
                <RelativeLayout
                    android:id="@+id/card_admistor_student"
                    android:background="@drawable/hover_effect"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"/>
                <ImageView
                android:layout_marginTop="-10dp"
                android:layout_gravity="center"
                android:src="@drawable/add_student"
                android:layout_width="35dp"
                android:layout_height="35dp"/>
                <TextView
                    android:textColor="#394D71"
                    android:textSize="14sp"
                    android:layout_marginTop="30dp"
                    android:layout_gravity="center"
                    android:textStyle="bold"
                    android:text="Add Student"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"/>
            </androidx.cardview.widget.CardView>
            <androidx.cardview.widget.CardView
                app:cardElevation="10dp"
                android:layout_marginHorizontal="5dp"
                android:layout_width="0dp"
                android:layout_weight="1"
                android:layout_height="match_parent">
                <RelativeLayout
                    android:id="@+id/card_homework"
                    android:background="@drawable/hover_effect"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"/>
                <ImageView
                    android:layout_gravity="center"
                    android:src="@drawable/coming_soon"
                    android:layout_width="40dp"
                    android:layout_height="40dp"/>
<!--                <TextView-->
<!--                    android:textColor="#394D71"-->
<!--                    android:textSize="14sp"-->
<!--                    android:layout_marginTop="30dp"-->
<!--                    android:layout_gravity="center"-->
<!--                    android:textStyle="bold"-->
<!--                    android:text=""-->
<!--                    android:layout_width="wrap_content"-->
<!--                    android:layout_height="wrap_content"/>-->
            </androidx.cardview.widget.CardView>
        </LinearLayout>
    </LinearLayout>
</LinearLayout>