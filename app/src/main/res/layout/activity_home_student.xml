<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    android:background="#E0E0E0"
    tools:context=".HomeStudent">
    <com.google.android.material.appbar.AppBarLayout
        android:id="@+id/navigation_click"
        android:layout_width="match_parent"
        android:elevation="10dp"
        android:layout_height="55dp">
        <androidx.appcompat.widget.Toolbar
            android:elevation="10dp"
            android:background="#F2472C"
            android:id="@+id/toolbar"
            android:layout_width="match_parent"
            android:layout_height="match_parent">
            <RelativeLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent">
                <RelativeLayout
                    android:layout_centerVertical="true"
                    android:background="@drawable/logo_border"
                    android:id="@+id/image"
                    android:layout_width="25dp"
                    android:layout_height="25dp">

                    <ImageView
                        android:layout_width="15dp"
                        android:layout_height="15dp"
                        android:layout_centerInParent="true"
                        android:src="@drawable/book"
                        tools:ignore="ContentDescription" />
                </RelativeLayout>

                <TextView
                    android:letterSpacing="0.04"
                    android:id="@+id/text"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="10dp"
                    android:layout_marginTop="10dp"
                    android:layout_toEndOf="@+id/image"
                     android:textStyle="bold"
                    android:text="mySchool"
                    android:textColor="@color/white"
                    android:textSize="18sp"
                    tools:ignore="HardcodedText" />

                <TextView
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_below="@+id/text"
                    android:layout_marginStart="10dp"
                    android:layout_toEndOf="@+id/image"
                    android:text="Your Digital Manager"
                    android:textAllCaps="true"
                    android:textColor="@color/white"
                    android:textSize="9sp"
                    android:textStyle="bold"
                    tools:ignore="SmallSp,TextContrastCheck" />

                <TextView
                    android:id="@+id/sesion_year"
                    android:layout_width="wrap_content"
                    android:layout_height="match_parent"
                    android:layout_alignParentEnd="true"
                    android:layout_marginEnd="30dp"
                    android:fontFamily="@font/actor"
                    android:gravity="center"
                    android:text="2021-2022"
                    android:textColor="@color/white"
                    android:textSize="14sp"
                    tools:ignore="HardcodedText,SmallSp,TextContrastCheck" />
            </RelativeLayout>
        </androidx.appcompat.widget.Toolbar>
    </com.google.android.material.appbar.AppBarLayout>
<androidx.core.widget.NestedScrollView
    android:layout_width="match_parent"
    android:layout_height="match_parent">
    <LinearLayout
        android:orientation="vertical"
        android:layout_width="match_parent"
        android:layout_height="match_parent">
        <com.denzcoskun.imageslider.ImageSlider
            android:background="@color/white"
            android:layout_marginHorizontal="5dp"
            android:scrollbarSize="10dp"
            app:iss_error_image="@drawable/error"
            app:iss_selected_dot="@drawable/selected_dot"
            app:iss_unselected_dot="@drawable/unselected_dot"
            android:id="@+id/image_slider"
            android:layout_width="match_parent"
            android:layout_height="240dp"
            app:iss_auto_cycle="true"
            app:iss_period="1000"
            app:iss_delay="100"
            app:iss_title_background="@drawable/slider_title_back"/>
    <TextView
        android:textStyle="bold"
        android:paddingBottom="10dp"
        android:paddingTop="10dp"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:fontFamily="@font/adamina"
        android:paddingStart="20dp"
        android:text="Students"
        android:textColor="@color/black"
        android:textSize="18sp"
        tools:ignore="RtlSymmetry" />
    <LinearLayout
        android:orientation="vertical"
       android:weightSum="3"
        android:layout_width="match_parent"
        android:layout_height="300dp">
        <LinearLayout
            android:elevation="10dp"
            android:orientation="horizontal"
            android:layout_weight="1"
            android:layout_width="match_parent"
            android:weightSum="3"
            android:layout_height="0dp">
            <androidx.cardview.widget.CardView
                app:cardElevation="0dp"
                android:layout_marginHorizontal="3dp"
                android:layout_width="0dp"
                android:layout_weight="1"
                android:layout_height="match_parent"
                tools:ignore="NestedWeights">
                <RelativeLayout
                    android:id="@+id/card_subject"
                    android:background="@drawable/hover_effect"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"/>
                <ImageView
                    android:layout_marginTop="-10dp"
                    android:layout_gravity="center"
                    android:src="@drawable/subject"
                    android:layout_width="35dp"
                    android:layout_height="35dp"
                    tools:ignore="ContentDescription" />
                <TextView
                    android:layout_marginStart="-3dp"
                    android:textColor="#394D71"
                    android:textSize="14sp"
                    android:layout_marginTop="30dp"
                    android:layout_gravity="center"
                    android:textStyle="bold"
                    android:text="Subject"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"/>
            </androidx.cardview.widget.CardView>
            <androidx.cardview.widget.CardView
                app:cardElevation="0dp"
                android:layout_marginHorizontal="3dp"
                android:layout_width="0dp"
                android:layout_weight="1"
                android:layout_height="match_parent">
                <RelativeLayout
                    android:id="@+id/card_syllabus"
                    android:background="@drawable/hover_effect"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"/>
                <ImageView
                    android:layout_marginTop="-10dp"
                    android:layout_gravity="center"
                    android:src="@drawable/syllabus"
                    android:layout_width="40dp"
                    android:layout_height="40dp"
                    tools:ignore="ContentDescription" />
                <TextView
                    android:textColor="#394D71"
                    android:textSize="14sp"
                    android:layout_marginTop="30dp"
                    android:layout_gravity="center"
                    android:textStyle="bold"
                    android:text="Syllabus"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"/>
            </androidx.cardview.widget.CardView>
            <androidx.cardview.widget.CardView
                app:cardElevation="0dp"
                android:layout_marginHorizontal="3dp"
                android:layout_width="0dp"
                android:layout_weight="1"
                android:layout_height="match_parent">
                <RelativeLayout
                    android:id="@+id/card_timetable"
                    android:background="@drawable/hover_effect"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"/>
                <ImageView
                    android:layout_marginTop="-10dp"
                    android:layout_gravity="center"
                    android:src="@drawable/calendar"
                    android:layout_width="35dp"
                    android:layout_height="35dp"
                    tools:ignore="ContentDescription" />
                <TextView
                    android:textColor="#394D71"
                    android:textSize="14sp"
                    android:layout_marginTop="30dp"
                    android:layout_gravity="center"
                    android:textStyle="bold"
                    android:text="Timetable"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"/>
            </androidx.cardview.widget.CardView>
        </LinearLayout>
        <LinearLayout
            android:elevation="10dp"
            android:layout_marginTop="8dp"
            android:layout_weight="1"
            android:layout_width="match_parent"
            android:weightSum="3"
            android:layout_height="0dp"
            android:orientation="horizontal"
           >
            <androidx.cardview.widget.CardView
                app:cardElevation="0dp"
                android:layout_marginHorizontal="3dp"
                android:layout_width="0dp"
                android:layout_weight="1"
                android:layout_height="match_parent"
                tools:ignore="NestedWeights">
                <RelativeLayout
                    android:id="@+id/card_list"
                    android:background="@drawable/hover_effect"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"/>
                <ImageView
                    android:layout_marginTop="-10dp"
                    android:layout_gravity="center"
                    android:src="@drawable/stusent_list"
                    android:layout_width="40dp"
                    android:layout_height="40dp"
                    tools:ignore="ContentDescription" />
                <TextView
                    android:textColor="#394D71"
                    android:textSize="14sp"
                    android:layout_marginTop="30dp"
                    android:layout_gravity="center"
                    android:textStyle="bold"
                    android:text="Student List"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"/>
            </androidx.cardview.widget.CardView>
            <androidx.cardview.widget.CardView
                app:cardElevation="0dp"
                android:layout_marginHorizontal="3dp"
                android:layout_width="0dp"
                android:layout_weight="1"
                android:layout_height="match_parent">
                <RelativeLayout
                    android:id="@+id/card_attendance"
                    android:background="@drawable/hover_effect"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"/>
                <ImageView
                    android:layout_marginTop="-10dp"
                    android:layout_gravity="center"
                    android:src="@drawable/attendance"
                    android:layout_width="40dp"
                    android:layout_height="40dp"
                    tools:ignore="ContentDescription" />
                <TextView
                    android:textColor="#394D71"
                    android:textSize="14sp"
                    android:layout_marginTop="30dp"
                    android:layout_gravity="center"
                    android:textStyle="bold"
                    android:text="Attendance"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"/>
            </androidx.cardview.widget.CardView>
            <androidx.cardview.widget.CardView
                app:cardElevation="0dp"
                android:layout_marginHorizontal="3dp"
                android:layout_width="0dp"
                android:layout_weight="1"
                android:layout_height="match_parent">
                <RelativeLayout
                    android:id="@+id/card_homework"
                    android:background="@drawable/hover_effect"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"/>

                <ImageView
                    android:layout_width="40dp"
                    android:layout_height="40dp"
                    android:layout_gravity="center"
                    android:layout_marginTop="-10dp"
                    android:src="@drawable/homework"
                    tools:ignore="ContentDescription,ImageContrastCheck" />
                <TextView
                    android:textColor="#394D71"
                    android:textSize="14sp"
                    android:layout_marginTop="30dp"
                    android:layout_gravity="center"
                    android:textStyle="bold"
                    android:text="Homework"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"/>
            </androidx.cardview.widget.CardView>
        </LinearLayout>
        <LinearLayout
            android:elevation="10dp"
            android:layout_marginTop="8dp"
            android:layout_weight="1"
            android:layout_width="match_parent"
            android:weightSum="3"
            android:layout_height="0dp"
            android:orientation="horizontal"
            >
            <androidx.cardview.widget.CardView
                app:cardElevation="0dp"
                android:layout_marginHorizontal="3dp"
                android:layout_width="0dp"
                android:layout_weight="1"
                android:layout_height="match_parent"
                tools:ignore="NestedWeights">
                <RelativeLayout
                    android:id="@+id/card_ICard"
                    android:background="@drawable/hover_effect"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"/>

                <ImageView
                    android:layout_width="40dp"
                    android:layout_height="40dp"
                    android:layout_gravity="center"
                    android:layout_marginTop="-10dp"
                    android:src="@drawable/id_card"
                    tools:ignore="ContentDescription,ImageContrastCheck" />
                <TextView
                    android:textColor="#394D71"
                    android:textSize="14sp"
                    android:layout_marginTop="30dp"
                    android:layout_gravity="center"
                    android:textStyle="bold"
                    android:text="ICard"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"/>
            </androidx.cardview.widget.CardView>
            <androidx.cardview.widget.CardView
                app:cardElevation="0dp"
                android:layout_marginHorizontal="3dp"
                android:layout_width="0dp"
                android:layout_weight="1"
                android:layout_height="match_parent">
                <RelativeLayout
                    android:id="@+id/card_t_list"
                    android:background="@drawable/hover_effect"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"/>
                <ImageView
                    android:layout_marginTop="-10dp"
                    android:layout_gravity="center"
                    android:src="@drawable/teacher"
                    android:layout_width="40dp"
                    android:layout_height="40dp"
                    tools:ignore="ContentDescription" />
                <TextView
                    android:textColor="#394D71"
                    android:textSize="14sp"
                    android:layout_marginTop="30dp"
                    android:layout_gravity="center"
                    android:textStyle="bold"
                    android:text="Teacher List"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"/>
            </androidx.cardview.widget.CardView>
            <androidx.cardview.widget.CardView
                app:cardElevation="0dp"
                android:layout_marginHorizontal="3dp"
                android:layout_width="0dp"
                android:layout_weight="1"
                android:layout_height="match_parent">
                <RelativeLayout
                    android:id="@+id/card_chat_bot"
                    android:background="@drawable/hover_effect"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"/>

                <ImageView
                    android:layout_width="35dp"
                    android:layout_height="35dp"
                    android:layout_gravity="center"
                    android:layout_marginTop="-10dp"
                    android:src="@drawable/chatbot"
                    tools:ignore="ContentDescription,ImageContrastCheck" />
                <TextView
                    android:textColor="#394D71"
                    android:textSize="14sp"
                    android:layout_marginTop="30dp"
                    android:layout_gravity="center"
                    android:textStyle="bold"
                    android:text="Chat Solution"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"/>
            </androidx.cardview.widget.CardView>
        </LinearLayout>
    </LinearLayout>
    <TextView
        android:textStyle="bold"
        android:paddingBottom="10dp"
        android:paddingTop="10dp"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:fontFamily="@font/adamina"
        android:paddingStart="20dp"
        android:text="Other"
        android:textColor="@color/black"
        android:textSize="18sp"
        tools:ignore="RtlSymmetry" />
    <LinearLayout
        android:layout_marginBottom="10dp"
        android:orientation="vertical"
        android:weightSum="3"
        android:layout_width="match_parent"
        android:layout_height="300dp">
        <LinearLayout
            android:orientation="horizontal"
            android:layout_weight="1"
            android:layout_width="match_parent"
            android:weightSum="3"
            android:layout_height="0dp">
            <androidx.cardview.widget.CardView
                app:cardElevation="0dp"
                android:layout_marginHorizontal="3dp"
                android:layout_width="0dp"
                android:layout_weight="1"
                android:layout_height="match_parent"
                tools:ignore="NestedWeights">
                <RelativeLayout
                    android:id="@+id/card_gallery"
                    android:background="@drawable/hover_effect"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"/>
                <ImageView
                    android:layout_marginTop="-10dp"
                    android:layout_gravity="center"
                    android:src="@drawable/gallery"
                    android:layout_width="35dp"
                    android:layout_height="35dp"
                    tools:ignore="ContentDescription" />
                <TextView
                    android:textColor="#394D71"
                    android:textSize="14sp"
                    android:layout_marginTop="30dp"
                    android:layout_gravity="center"
                    android:textStyle="bold"
                    android:text="Gallery"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"/>
            </androidx.cardview.widget.CardView>
            <androidx.cardview.widget.CardView
                app:cardElevation="0dp"
                android:layout_marginHorizontal="3dp"
                android:layout_width="0dp"
                android:layout_weight="1"
                android:layout_height="match_parent">
                <RelativeLayout
                    android:id="@+id/card_complaint"
                    android:background="@drawable/hover_effect"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"/>
                <ImageView
                    android:layout_marginTop="-10dp"
                    android:layout_gravity="center"
                    android:src="@drawable/notic"
                    android:layout_width="38dp"
                    android:layout_height="38dp"
                    tools:ignore="ContentDescription" />
                <TextView
                    android:textColor="#394D71"
                    android:textSize="14sp"
                    android:layout_marginTop="30dp"
                    android:layout_gravity="center"
                    android:textStyle="bold"
                    android:text="Complaint"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"/>
            </androidx.cardview.widget.CardView>
            <androidx.cardview.widget.CardView
                app:cardElevation="0dp"
                android:layout_marginHorizontal="3dp"
                android:layout_width="0dp"
                android:layout_weight="1"
                android:layout_height="match_parent">
                <RelativeLayout
                    android:id="@+id/card_events"
                    android:background="@drawable/hover_effect"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"/>
                <ImageView
                    android:layout_marginTop="-10dp"
                    android:layout_gravity="center"
                    android:src="@drawable/event"
                    android:layout_width="35dp"
                    android:layout_height="35dp"
                    tools:ignore="ContentDescription" />
                <TextView
                    android:textColor="#394D71"
                    android:textSize="14sp"
                    android:layout_marginTop="30dp"
                    android:layout_gravity="center"
                    android:textStyle="bold"
                    android:text="Events"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"/>
            </androidx.cardview.widget.CardView>
        </LinearLayout>
        <LinearLayout
            android:layout_marginTop="8dp"
            android:layout_weight="1"
            android:layout_width="match_parent"
            android:weightSum="3"
            android:layout_height="0dp"
            android:orientation="horizontal"
            >
            <androidx.cardview.widget.CardView
                app:cardElevation="0dp"
                android:layout_marginHorizontal="3dp"
                android:layout_width="0dp"
                android:layout_weight="1"
                android:layout_height="match_parent"
                tools:ignore="NestedWeights">
                <RelativeLayout
                    android:id="@+id/card_application"
                    android:background="@drawable/hover_effect"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"/>
                <ImageView
                    android:layout_marginTop="-10dp"
                    android:layout_gravity="center"
                    android:src="@drawable/apllication"
                    android:layout_width="35dp"
                    android:layout_height="35dp"
                    tools:ignore="ContentDescription" />
                <TextView
                    android:textColor="#394D71"
                    android:textSize="14sp"
                    android:layout_marginTop="30dp"
                    android:layout_gravity="center"
                    android:textStyle="bold"
                    android:text="Application"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"/>
            </androidx.cardview.widget.CardView>
            <androidx.cardview.widget.CardView
                app:cardElevation="0dp"
                android:layout_marginHorizontal="3dp"
                android:layout_width="0dp"
                android:layout_weight="1"
                android:layout_height="match_parent">
                <RelativeLayout
                    android:id="@+id/card_std_materials"
                    android:background="@drawable/hover_effect"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"/>
                <ImageView
                    android:layout_marginTop="-10dp"
                    android:layout_gravity="center"
                    android:src="@drawable/study"
                    android:layout_width="35dp"
                    android:layout_height="35dp"
                    tools:ignore="ContentDescription" />
                <TextView
                    android:textColor="#394D71"
                    android:textSize="14sp"
                    android:layout_marginTop="30dp"
                    android:layout_gravity="center"
                    android:textStyle="bold"
                    android:text="Std. Materials"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"/>
            </androidx.cardview.widget.CardView>
            <androidx.cardview.widget.CardView
                app:cardElevation="0dp"
                android:layout_marginHorizontal="3dp"
                android:layout_width="0dp"
                android:layout_weight="1"
                android:layout_height="match_parent">
                <RelativeLayout
                    android:id="@+id/card_video"
                    android:background="@drawable/hover_effect"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"/>
                <ImageView
                    android:layout_marginTop="-10dp"
                    android:layout_gravity="center"
                    android:src="@drawable/video"
                    android:layout_width="35dp"
                    android:layout_height="35dp"
                    tools:ignore="ContentDescription,TooManyViews" />
                <TextView
                    android:textColor="#394D71"
                    android:textSize="14sp"
                    android:layout_marginTop="30dp"
                    android:layout_gravity="center"
                    android:textStyle="bold"
                    android:text="Video"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"/>
            </androidx.cardview.widget.CardView>
        </LinearLayout>
        <LinearLayout
            android:layout_marginTop="8dp"
            android:layout_weight="1"
            android:layout_width="match_parent"
            android:weightSum="3"
            android:layout_height="0dp"
            android:orientation="horizontal"
            >
            <androidx.cardview.widget.CardView
                app:cardElevation="0dp"
                android:layout_marginHorizontal="3dp"
                android:layout_width="0dp"
                android:layout_weight="1"
                android:layout_height="match_parent"
                tools:ignore="NestedWeights">
                <RelativeLayout
                    android:id="@+id/card_result"
                    android:background="@drawable/hover_effect"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"/>
                <ImageView
                    android:layout_marginTop="-8dp"
                    android:layout_gravity="center"
                    android:src="@drawable/chat"
                    android:layout_width="35dp"
                    android:layout_height="35dp"
                    tools:ignore="ContentDescription" />
                <TextView
                    android:textColor="#394D71"
                    android:textSize="14sp"
                    android:layout_marginTop="30dp"
                    android:layout_gravity="center"
                    android:textStyle="bold"
                    android:text="Chats"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"/>
            </androidx.cardview.widget.CardView>
            <androidx.cardview.widget.CardView
                app:cardElevation="0dp"
                android:layout_marginHorizontal="3dp"
                android:layout_width="0dp"
                android:layout_weight="1"
                android:layout_height="match_parent">
                <RelativeLayout
                    android:id="@+id/card_notice"
                    android:background="@drawable/hover_effect"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"/>
                <ImageView
                    android:layout_marginTop="-10dp"
                    android:layout_gravity="center"
                    android:src="@drawable/notice"
                    android:layout_width="35dp"
                    android:layout_height="35dp"
                    tools:ignore="ContentDescription" />
                <TextView
                    android:textColor="#394D71"
                    android:textSize="14sp"
                    android:layout_marginTop="30dp"
                    android:layout_gravity="center"
                    android:textStyle="bold"
                    android:text="Notice"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"/>
            </androidx.cardview.widget.CardView>
            <androidx.cardview.widget.CardView
                app:cardElevation="0dp"
                android:layout_marginHorizontal="3dp"
                android:layout_width="0dp"
                android:layout_weight="1"
                android:layout_height="match_parent">
                <RelativeLayout
                    android:id="@+id/card_assignment"
                    android:background="@drawable/hover_effect"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"/>
                <ImageView
                    android:layout_marginTop="-10dp"
                    android:layout_gravity="center"
                    android:src="@drawable/assignment"
                    android:layout_width="35dp"
                    android:layout_height="35dp"
                    tools:ignore="ContentDescription" />
                <TextView
                    android:textColor="#394D71"
                    android:textSize="14sp"
                    android:layout_marginTop="30dp"
                    android:layout_gravity="center"
                    android:textStyle="bold"
                    android:text="Assignment"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"/>
            </androidx.cardview.widget.CardView>
        </LinearLayout>
    </LinearLayout>
    </LinearLayout>
</androidx.core.widget.NestedScrollView>
</LinearLayout>